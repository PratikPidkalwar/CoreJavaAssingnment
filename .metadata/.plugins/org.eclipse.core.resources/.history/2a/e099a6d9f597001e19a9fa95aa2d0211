package practiceTodo6;

public class CourseCollect {

	private static Course[] allCourse=new Course[2];
	static {
		allCourse[0]=new Course(1,"abc");
		allCourse[1]=new Course(2,"xyz");
	}
	
	public void getMatchCourses(String name) {
		
		for(Course cc:allCourse) {
			String currProviderCourse=cc.getName();
			
			if(currProviderCourse.equals(name)) {
				System.out.println(cc);
			}
		}
	}
	
}
